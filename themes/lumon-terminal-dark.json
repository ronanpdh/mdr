      "settings": {
        "foreground": "#60A0FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#4DCBEE",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#4DCBEE"
      }
    },
    {
      "scope": "entity.name.tag",
